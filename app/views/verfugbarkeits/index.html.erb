<% if notice %>
  <p id="notice"><%= notice %></p>
<% end %>
<h1>Verfugbarkeits</h1>
<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @verfugbarkeits.each do |verfugbarkeit| %>
      <tr>
        <td><%= verfugbarkeit.status %></td>
        <td><%= verfugbarkeit.start %></td>
        <td><%= verfugbarkeit.ende %></td>
        <td><%= link_to 'Show', verfugbarkeit %></td>
        <td><%= link_to 'Edit', edit_verfugbarkeit_path(verfugbarkeit) %></td>
        <td><%= link_to 'Destroy', verfugbarkeit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Verfugbarkeit', new_verfugbarkeit_path %>